{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "sh -c 'npx convex dev & next dev --turbopack'",
    "dev:convex": "npx convex dev",
    "dev:next": "next dev --turbopack",
    "build": "npm run type-check && next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:all": "next lint",
    "type-check": "tsc --noEmit --skipLibCheck",
    "type-check:all": "tsc --noEmit --skipLibCheck",
    "docker:build": "docker build -t fastbite .",
    "docker:run": "docker run -p 3000:3000 fastbite",
    "docker:dev": "docker run -p 3000:3000 -v $(pwd):/app -v /app/node_modules fastbite npm run dev",
    "docker:stop": "docker stop $(docker ps -q --filter ancestor=fastbite)",
    "docker:logs": "docker logs $(docker ps -q --filter ancestor=fastbite) -f",
    "docker:clean": "docker system prune -f && docker image rm fastbite || true",
    "docker:push": "docker tag fastbite your-registry/fastbite:latest && docker push your-registry/fastbite:latest",
    "docker:pull": "docker pull your-registry/fastbite:latest",
    "compose:up": "docker-compose up -d",
    "compose:down": "docker-compose down",
    "compose:build": "docker-compose build",
    "compose:logs": "docker-compose logs -f",
    "compose:dev": "docker-compose -f docker-compose.dev.yml up",
    "compose:dev:build": "docker-compose -f docker-compose.dev.yml up --build",
    "compose:prod": "docker-compose up -d",
    "compose:prod:build": "docker-compose up --build -d",
    "compose:paystack": "docker-compose --profile paystack-testing up -d",
    "compose:paystack:build": "docker-compose --profile paystack-testing up --build -d",
    "db:migrate": "npx convex deploy",
    "db:dev": "npx convex dev",
    "convex:dev": "npx convex dev",
    "convex:deploy": "npx convex deploy",
    "convex:logs": "npx convex logs --history 50"
  },
